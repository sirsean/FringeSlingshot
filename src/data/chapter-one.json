    "gastrotech-3": {
      "startScreen": "screen1",
      "screens": {
        "screen1": {
          "background": "/img/bar/background1.png",
          "bartop": "/img/bar/bartop1.png",
          "character": "/img/characters/drifter06/drifter06-looking-into-cam.png",
          "text": "He's covering tracks, no witnesses. He must know that I discovered something priceless, something they'd kill for.",
