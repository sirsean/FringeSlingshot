    "religious-pilgrim": {
      "startScreen": "screen1",
      "screens": {
        "screen1": {
          "background": "/img/bar/background1.png",
          "bartop": "/img/bar/bartop1.png",
          "character": "/img/characters/religious-pilgrim/pilgrim.png",
          "text": "Sin hides in the penumbra of the Fringe. The Lighthouse Sept spread the word of Tupper the Younger.",
